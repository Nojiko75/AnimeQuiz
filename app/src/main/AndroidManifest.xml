<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tanoshi.nojiko.animequiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameActivity" android:screenOrientation="portrait" />
        <activity android:name=".RankingActivity" android:screenOrientation="portrait"/>
        <activity android:name=".DoneActivity" android:screenOrientation="portrait"/>
        <activity android:name=".ChoiceGameActivity" android:screenOrientation="portrait"/>
        <activity android:name=".EasyPersoQuizActivity" android:screenOrientation="portrait"/>
        <activity android:name=".MediumPersoQuizActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>